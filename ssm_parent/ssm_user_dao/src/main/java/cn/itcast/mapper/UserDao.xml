<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itcast.dao.UserDao">
    <insert id="saveUser" parameterType="cn.itcast.domain.Users">
        INSERT INTO users(username,password,telephone,state) VALUES(#{username},#{password},#{telephone},#{state})
    </insert>

</mapper>